{
  "name": "mfa-telephone-analysis",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:docker:db": "docker compose --env-file ./environments/local.env -f docker-compose.db.yaml up --build",
    "start:local": "nest start --watch -e \"node --env-file ./environments/local.env\"",
    "start:dev": "nest start --env-file ./environments/dev.env",
    "start:prod": "node dist/main --env-file ./environments/prod.env",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --config ./jest-unit.json",
    "prepare": "husky",
    "typeorm": "typeorm-ts-node-commonjs --dataSource ./ormconfig.ts",
    "migration:create": "cross-var npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
    "migration:generate": "cross-var npm run typeorm -- migration:generate ./src/database/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:run": "npm run typeorm -- migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/nestjs": "^8.26.0",
    "@sentry/profiling-node": "^8.26.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compromise": "^14.14.0",
    "compromise-stats": "^0.1.0",
    "cross-var": "^1.1.0",
    "ffmpeg-static": "^5.2.0",
    "file-type": "^16.5.4",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.4.1",
    "mp3-duration": "^1.1.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^8.0.1",
    "node-lame": "^1.3.2",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sentiment": "^5.0.2",
    "speech-to-text": "^2.9.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "vosk": "^0.3.39",
    "wav-decoder": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.21",
    "@types/file-type": "^10.6.0",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.0",
    "@types/sentiment": "^5.0.4",
    "@types/supertest": "^6.0.2",
    "@types/three": "^0.169.0",
    "@types/uuid": "^10.0.0",
    "@types/vosk": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": "eslint --fix"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
